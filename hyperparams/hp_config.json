{
    "Env": {
        "name": "donkey-generated-track-v0"
    },

    "Agent": {
        "name": "SAC",
        "ACTION_SIZE": 2,
        "HIDDEN_SIZE": 512,
        "MIN_SIZE": 100,
        "BATCH_SIZE": 256,
        "MAX_ACTION": 0.8,
        "TEMPERATURE": 0.2,
        "TAU": 0.005,
        "GAMMA": 0.99,
        "USE_NOISE": true
    },

    "env_wrapper": {
        "SIGMA": 0.2,
        "ACTION_COST": 0.5,
        "TARGET_SPEED": 1.0
    },    

    "action_wrapper": {
        "method": "weighted_moving_average",
        "window_size": 5,
        "weights": [0.1, 0.2, 0.3, 0.4],
        "alpha": 0.6
    },

    "observation_processing": {
        "method": "normalization",
        "parameters": {
            "mean": [0.5, 0.5],
            "std": [0.1, 0.1]
        }
    }
}